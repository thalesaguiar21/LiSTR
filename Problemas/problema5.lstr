proc mdc(int n, int m, inout int r){
	if(m % n == 0){
		r = n;
	}
	if(n % m == 0){
		r = m;
	}
	else{
		if(m > n){
			mdc(n, m % n, r);
		}
		else{
			mdc(m, n % m, r);
		}
	}
}

proc main(){
	int m, n, r = 0;
	write "Digite dois numeros inteiros: \n";
	read m n;
	mdc(m, n, r);
	write "O mdc de " m " e " n " eh " r "\n"; 
}